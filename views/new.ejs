<%-include("partials/header")-%>
<div class="container">

<h1 class="text-center p-2">Create a New Photo</h1>
<form action="/campgrounds" method="post" enctype="multipart/form-data" >
  <div class="" style="max-width: 330px; padding: 15px; margin:auto; "> 	
	<div class="form-group "  > 
	  <input type="text" name="name" placeholder="name" class="form-control mb-3" autocomplete="off">
	 <!--  <input type="text" name="image" placeholder="imag url" class="form-control mb-3" > -->
	  <input type="file" name="file" placeholder="imag url" class="form-control mb-3" autocomplete="off">
	   <input type="text" name="desc" placeholder="description" class="form-control mb-3" autocomplete="off">
	</div>
	<button name="button" type="Submit" class="btn btn-primary btn-block">Submit</button>
  </div>
</form>
</div>


<%-include("partials/footer")-%>